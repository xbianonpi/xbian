<!-- http://etext.virginia.edu/tei/iso639.html          -->

<language locale="ja">
  <charsets>
    <gui unicodefont="true">CP932</gui>
    <subtitle>CP932</subtitle>
  </charsets>

  <dvd>
    <menu>ja</menu>
    <audio>ja</audio>
    <subtitle>ja</subtitle>
  </dvd>

  <regions>
    <region name="Japan" locale="JP">
      <dateshort>YYYY/MM/DD</dateshort>
      <datelong>YYYY'年'M'月'D'日' DDDD</datelong>
      <time symbolAM="AM" symbolPM="PM">h:mm:ss xx</time>
      <tempunit>C</tempunit>
      <speedunit>kmh</speedunit>
      <timezone>JST</timezone>
    </region> 
  </regions>

  <sorttokens>
    <token>The</token>
  </sorttokens>
</language>
